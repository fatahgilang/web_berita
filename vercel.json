{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "outputDirectory": "public",
  "build": {
    "env": {
      "NODE_VERSION": "20"
    }
  },
  "github": {
    "silent": true
  },
  "regions": ["iad1"]
}